{
  "imports": {
    "@std/fs": "jsr:@std/fs@1",
    "@std/fs/ensure-symlink": "jsr:@std/fs@1/ensure-symlink",
    "@std/fs/ensure-dir": "jsr:@std/fs@1/ensure-dir",
    "@std/fs/exists": "jsr:@std/fs@1/exists",
    "@std/log": "jsr:@std/log@0",
    "@std/path": "jsr:@std/path@1",
    "@std/assert": "jsr:@std/assert@1",
    "graphviz": "https://deno.land/x/graphviz@v0.2.1/mod.ts",
    "download": "https://deno.land/x/download@v1.0.0/mod.ts",
    "is_exe": "https://deno.land/x/is_exe@v1.0.3/mod.ts",
    "json_tree": "https://raw.githubusercontent.com/satty1987/json_tree/master/mod.ts",
    "open": "https://deno.land/x/open@v0.0.5/index.ts"
  },
  "tasks": {
    "verify": "deno check && deno lint && deno task test",
    "test": "deno test --allow-read --allow-write tests/",
    "example-depgraph": "deno run --allow-read --allow-write --allow-run examples/example-dep-graph.ts",
    "example-print": "deno run --allow-read --allow-write --allow-run examples/example-print.ts",
    "example-dryrun": "deno run --allow-read --allow-write --allow-run examples/example-dry-run.ts",
    "example-stats": "deno run --allow-read --allow-write --allow-run examples/example-stats.ts"
  }
}
